    <script type="text/javascript">
		$(document).ready(function(){				
			var modalID, modalIDOpen;
			$('.link-form > a').click(function(e){
				e.preventDefault();
				modalID = "#" + $(this).parents('.modal').attr('id'),
				modalIDOpen = $(this).attr('href');
				$(modalID).modal('hide'); 	
			
				$(modalID).on('hidden.bs.modal', function () {
					setTimeout(function(){
						$(modalIDOpen).modal('show'); 
					}, 300);
				});
			});				
		});	
    </script> 
    
